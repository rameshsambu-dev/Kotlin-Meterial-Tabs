<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ettle.kotlinmeterialtabs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme" >
        <activity
            android:name=".acivity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".acivity.ScrollableTabsActivity"
            android:label="@string/title_activity_scrollable_tabs"
            android:parentActivityName=".acivity.MainActivity" >
        </activity>
        <activity
            android:name=".acivity.SimpleTabsActivity"
            android:label="@string/title_activity_simple_tabs"
            android:parentActivityName=".acivity.MainActivity" >
        </activity>
        <activity
            android:name=".acivity.IconTextTabsActivity"
            android:label="@string/title_activity_icon_text_tabs"
            android:parentActivityName=".acivity.MainActivity" >
        </activity>
        <activity
            android:name=".acivity.IconTabsActivity"
            android:label="@string/title_activity_icon_tabs"
            android:parentActivityName=".acivity.MainActivity" >
        </activity>
        <activity
            android:name=".acivity.CustomViewIconTextTabsActivity"
            android:label="@string/title_activity_custom_view_icon_text_tabs"
            android:parentActivityName=".acivity.MainActivity" >
        </activity>
    </application>

</manifest>